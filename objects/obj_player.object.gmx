            <string>var playerSpeed = 32;
    
if (keyboard_check(ord("D")) &amp;&amp; !place_meeting(x+playerSpeed, y, obj_wall) ) {
    x += playerSpeed;
    image_index = 0;
}
if (keyboard_check(ord("A")) &amp;&amp; !place_meeting(x-playerSpeed, y, obj_wall)) {
    x -= playerSpeed;
    image_index = 1;
}
if (keyboard_check(ord("W")) &amp;&amp; !place_meeting(x, y-playerSpeed, obj_wall)) {
    y -= playerSpeed;
}
if (keyboard_check(ord("S")) &amp;&amp; !place_meeting(x, y+playerSpeed, obj_wall)) {
    y += playerSpeed;
}
