   show_message(view_hport[0]);
}
if (keyboard_check(ord("R"))) {
   game_restart();
}

view_xview[0] = x - (view_wview[0] / 2);
view_yview[0] = y - (view_hview[0] / 2);

var x1 = floor(x/32);
var y1 = floor(y/32);

if (health &lt; 0) {
    show_message("Game over. You were killed.");
    game_restart();
}
